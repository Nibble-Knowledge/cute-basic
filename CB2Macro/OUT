INF 0x400
START:

INCL asm-library/Mult4.s
INCL asm-library/Mult8.s
INCL asm-library/Mult16.s








MOV A INTO A2unsigned[1]
LOD N_[0]
STR A2unsigned[0]
MOV8 A2unsigned INTO Mult8_Op1[1]
MOV8 B INTO Mult8_Op2[1]
MOVADDR Return30 INTO Mult8_RetAddr[1]
LOD N_[0]
STR Mult8_Op1Ex
STR Mult8_Op2Ex
JMP Mult8_UnsignedEntry
Return30:
NOP 0
MOV64 Mult8_Ans INTO E





END:
A: .data 1 0
B: .data 2 0
C: .data 4 0
D: .data 8 0
E: .data 16 0
A2unsigned: .data 2 0
