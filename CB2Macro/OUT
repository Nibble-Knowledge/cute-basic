START:
INCL asm-library/Mult4.s
INCL asm-library/Mult8.s
INCL asm-library/Mult16.s
INCL asm-library/Mult32.s
INCL asm-library/SignedCompare.s

#TEST function begins here
LOD N_[0]
JMP END
TESTEntry:
MOV TEST_C INTO TEST_C8unsigned[1]
LOD N_[0]
STR TEST_C8unsigned[0]
ADD8 TEST_B TEST_C8unsigned INTO TEST_D
MOV TEST_D INTO TEST_RetVal
LOD N_[0]
TEST_RetAddr:
JMP 0000




MOV B INTO TEST_B
MOV C INTO TEST_C
MOVADDR TEST_Return17 INTO TEST_RetAddr[1]
LOD N_[0]
JMP TESTEntry
TEST_Return17:
NOP 0
MOV TEST_RetVal INTO A







END:
TEST_RetVal: .data 8 0
TEST_B: .data 8 0
TEST_C: .data 4 0
TEST_D: .data 8 0
TEST_C8unsigned: .data 8 0
A: .data 8 0
B: .data 8 4
C: .data 4 1
