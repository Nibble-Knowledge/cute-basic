START:

MOV C INTO A
MOV N_[4] INTO B
LSHIFT D INTO A
LSHIFT D INTO A
LSHIFT D INTO A


ADD D N_[8] INTO F
ADD D E INTO F
MULT N_[2] N_[4] INTO A
ADD B A INTO C

MOV A INTO TEST_Param0
MOV C INTO TEST_Param1
MOV D INTO TEST_Param2
MOVADDR TEST_Return21 INTO TEST_RetAddr[1]
LOD N_[0]
JMP TESTEntry
TEST_Return21:
NOP 0
MOV TEST_RetVal INTO F

MOV N_[4] INTO TEST2_Param0
MOVADDR TEST2_Return23 INTO TEST2_RetAddr[1]
LOD N_[0]
JMP TEST2Entry
TEST2_Return23:
NOP 0

#TEST function begins here
LOD N_[0]
JMP END
TESTEntry:
MOV N_[2] INTO A
MOV A INTO TEST_RetVal
TEST_RetAddr:
JMP 0000

END:

A: .data 4 0
B: .data 1 0
C: .data 4 10
