START:

MOV C INTO A
MOV N_[4] INTO B
LSHIFT D INTO A
LSHIFT D INTO A
LSHIFT D INTO A


MOV TESTReturnLabel15 INTO TESTReturnLabel
LOD 0
JMP TESTLabel
TESTReturnLabel15:

#TEST function begins here
LOD 0
JMP END
TESTLabel:
MOV N_[2] INTO A
STR TESTReturnValue
LOD 0
JMP TESTReturnLabel[1]
LOD 0
JMP TESTReturnLabel

END:

########## DATA SECTION BELOW HERE ##########
A: .data 4 0
B: .data 1 0
C: .data 4 10
D: .data 8 100 Signed?

S: .ascii "S"
Z: .ascii "Z"
