START
NOHEADER
LET X BE 7
LET TRUE BE 1
LET FALSE BE 0
LET Y AS 11 BE 19
LET Z AS 2
X BE X AND Y
Y BE X ADD Y
Z BE COMPLEMENT Y
PRINT 7 UNSIGNED X
PRINT 2 CHAR CHAR A
IF FALSE
PRINT 2 CHAR CHAR B
PRINT 2 CHAR CHAR A
PRINT 2 CHAR CHAR D
ENDIF
IF TRUE
PRINT 2 CHAR CHAR G
PRINT 2 CHAR CHAR U
PRINT 2 CHAR CHAR D
ENDIF
PRINT 2 CHAR CHAR \n
LOOPWHILE TRUE
    X BE DECREMENT X
    PRINT 1 UNSIGNED X
    IF X
    ELSE
        EXITLOOP
    ENDIF
ENDLOOP
END
