START
CALL TESTERBEEP
END

FUNCTION TESTERBEEP

DEFINE TESTERBEEP
    LET OUT AS 2
    LET IN AS 2
    PRINT 2 CHAR CHAR T
    PRINT 2 CHAR CHAR O
    PRINT 2 CHAR CHAR F
    PRINT 2 CHAR CHAR I
    PRINT 2 CHAR CHAR L
    PRINT 2 CHAR CHAR E
    PRINT 2 CHAR CHAR :
    GETKEY OUT
    FILEOUT OUT FILETEST AT 0
    PRINT 2 CHAR CHAR F
    PRINT 2 CHAR CHAR R
    PRINT 2 CHAR CHAR O
    PRINT 2 CHAR CHAR M
    PRINT 2 CHAR CHAR F
    PRINT 2 CHAR CHAR I
    PRINT 2 CHAR CHAR L
    PRINT 2 CHAR CHAR E
    PRINT 2 CHAR CHAR :
    FILEIN IN FILETEST AT 0
    PRINT 2 CHAR IN
    PRINT 2 CHAR CHAR \n
ENDFUNCTION

