START
CALL TESTERBEEP
END

FUNCTION TESTERBEEP
    LET OUT AS 2 SIGNED
    LET IN AS 2 SIGNED
    # single line comment
	/#
	MULTILINE
	MUTHA
	TRUCKA
	#/
	PRINT 2 CHAR CHAR T
    PRINT 2 CHAR CHAR O
    PRINT 2 CHAR CHAR F
    PRINT 2 CHAR CHAR I
    PRINT 2 CHAR CHAR L
    PRINT 2 CHAR CHAR E
    PRINT 2 CHAR CHAR :
	PASSTHROC puts("derp");
    CALL GETCHAR OUT
  	PRINT 2 CHAR OUT
	CALL PUTCHAR OUT FILETEST
	CALL GETCHAR IN FILETEST
	CALL PUTCHAR IN
    PRINT 2 CHAR CHAR F
    PRINT 2 CHAR CHAR R
    PRINT 2 CHAR CHAR O
    PRINT 2 CHAR CHAR M
    PRINT 2 CHAR CHAR F
    PRINT 2 CHAR CHAR I
    PRINT 2 CHAR CHAR L
    PRINT 2 CHAR CHAR E
    PRINT 2 CHAR CHAR :
    FILEIN IN FILETEST AT 15
    PRINT 2 CHAR IN
    PRINT 2 CHAR CHAR \n
ENDFUNCTION

